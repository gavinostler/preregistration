{
  "name": "StateMachine",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "CoreLibs": {
        "$path": "src/packages",
        "$ignoreUnknownInstances": true
      },
      "Packages": {
        "$path": "Packages",
        "$ignoreUnknownInstances": true
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      },
      "ServerPackages": {
        "$path": "src/serverpackages",
        "$ignoreUnknownInstances": true
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      }
    }
  }
}
